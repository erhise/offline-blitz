{
  "name": "offline-blitz",
  "version": "1.0.0",
  "description": "Simple sandbox to run code locally",
  "main": ".lib/index.js",
  "scripts": {
    "build": "tsc -p .",
    "sandbox": "node ./lib/index.js",
    "vim": "node ./lib/scripts/run-vim.js vim",
    "nvim": "node ./lib/scripts/run-vim.js nvim",
    "copy:default": "node ./lib/scripts/copy.js app",
    "copy:rxjs": "node ./lib/scripts/copy.js rxjs",
    "copy:example": "node ./lib/scripts/copy.js example"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/erhise/offline-blitz.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/erhise/offline-blitz/issues"
  },
  "homepage": "https://github.com/erhise/offline-blitz#readme",
  "dependencies": {
    "chokidar": "^3.5.1",
    "rxjs": "^6.6.3",
    "ts-node": "^9.1.1",
    "typescript": "^4.2.2"
  },
  "devDependencies": {
    "@types/blessed": "^0.1.17",
    "@types/node": "^14.14.31",
    "blessed": "^0.1.81",
    "blessed-contrib": "^4.8.21"
  }
}
